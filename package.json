{
  "name": "enterprise-angular-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dashboard:serve": "ng serve dashboard -o --port 3000",
    "dashboard:build": "ng build dashboard --prod",
    "dashboard:watch-build": "ng build dashboard --prod --watch",
    "dashboard:serve-build": "cd projects/dashboard && node server.js",
    "custumer:serve": "ng serve custumer -o --port 3001",
    "custumer:build": "ng build custumer --prod",
    "custumer:watch-build": "ng build custumer --prod --watch",
    "custumer:serve-build": "cd projects/custumer && node server.js",
    "ui-layout:build": "ng build ui-layout && npm i dist/ui-layout --save-optional",
    "library:build": "npm run ui-layout:build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.3",
    "@angular/common": "~10.2.3",
    "@angular/compiler": "~10.2.3",
    "@angular/core": "~10.2.3",
    "@angular/forms": "~10.2.3",
    "@angular/platform-browser": "~10.2.3",
    "@angular/platform-browser-dynamic": "~10.2.3",
    "@angular/router": "~10.2.3",
    "core-js": "^2.5.4",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.3",
    "@angular/language-service": "~10.2.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "es6-shim": "^0.35.5",
    "express-serve-static-core": "^0.1.1",
    "fs-extra": "^8.0.1",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "moment": "^2.24.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  },
  "optionalDependencies": {
    "@ui/layout": "file:dist/ui-layout"
  }
}
